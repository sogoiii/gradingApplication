            //-   select.span2(name='Grade', size='1')
            //-     option grade 1
            //-     option grade 2
            //- .span2
            //-   select.span2(name='Subject', size='1')
            //-     option subject 1
            //-     option subject 2
          .control-group    
      .span2 
        h3 Placeholder Data
        p #{title}