!=partial('partials/_TopBarLoggedIn')
  .container-fluid
    .row-fluid
      .span2
        include partials/_SideBar
      .span8
        h1 This is the user Statistics page
          p #{title}
          if(typeof(testid) != 'undefined')
            input#testID(type='hidden', name='testID', value='#{testid}')
          if(typeof(Statdata) != 'undefined')
            p  statdatae exists
            != "<script type='text/javascript'>"
            != "var Correct = [];"
            != "var Incorrect = [];"
            each question in Statdata
              != "Correct.push(" + question.CorrectlyAnswered +");"
              != "Incorrect.push(" + question.IncorrectlyAnswered +");"
            != "</script>"  
          p#testout  
        #TRbyQuestion(style='height: 300px; width: 400px;')
        